<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
	"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<base href="{{base_url}}" />
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
		<title>{{#title}}{{title}} &ndash; {{/title}}Park a Lot</title>
		<meta name="description" content="Rutgers Software Engineering Project (Spring 2011)" />
		<meta name="keywords" content="parking, garage, automation" />
		<link rel="stylesheet" type="text/css" href="media/css/style.css" media="screen, projection" />
		<!--[if IE]><link rel="stylesheet" type="text/css" href="media/css/ie.css" media="screen, projection" /><![endif]-->
		<script type="text/javascript" src="media/js/jquery-1.5.1.min.js"></script>
		<script type="text/javascript" src="media/js/common.js"></script>
		{{#styles}}
			{{#condition}}<!--[if {{condition}}]>{{/condition}}<link rel="stylesheet" type="text/css" href="{{href}}" media="{{media}}" />{{#condition}}<![endif]-->{{/condition}}
		{{/styles}}
		{{#scripts}}
			<script type="text/javascript" src="{{src}}"></script>
		{{/scripts}}
		{{#inline_js}}
			<script type="text/javascript">{{inline_js}}</script>
		{{/inline_js}}
	</head>
	<body>
		{{>notifications}}
		<div id="header-wrapper" class="wrapper">
			<div id="header" class="container">
				<a href="" id="logo"></a>
				<ul id="topnav" class="navbar clearfix">
					<li><a href="/">Home</a></li>
					<li><a href="reservation/create">Reserve</a></li>
					{{#logged_in}}
					<li><a href="admin/garage/usage">Admin</a></li>
					<li><a href="user/logout">Logout</a></li>
					{{/logged_in}}
					{{^logged_in}}
					<li><a href="user/login">Login</a></li>
					<li><a href="user/register">Register</a></li>
					{{/logged_in}}
				</ul>
			</div>
		</div>
		<div id="main-wrapper" class="wrapper">
			<div id="main" class="container">
				{{>content}}
			</div>
		</div>
		<div id="footer-wrapper" class="wrapper">
			<div id="footer" class="container">
				<span>&copy; Park a Lot Inc. 2010 - 2011</span>
				<ul class="navbar cleafix">
					<li><a href="">Help</a></li>
					<li><a href="">Contact</a></li>
					<li><a href="">About Us</a></li>
				</ul>
			</div>
		</div>
		{{&profiler}}
	</body>
</html>